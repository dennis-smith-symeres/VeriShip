{

  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "qualified.domain.name",
    "TenantId": "",
    "ClientId": "",
    "KeyVaultName": "",
    "CertificateThumbprint": "",
    "ClientCertificates": [
      {
        "SourceType": "StoreWithThumbprint",
        "CertificateStorePath": "CurrentUser/My",
        "CertificateThumbprint": ""
      }
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.IdentityModel": "Error",
        "System.Net.Http.HttpClient": "Error",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "Microsoft.Identity.Web.TokenAcquisition": "Error"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "Destructure": [
      { "Name": "ToMaximumDepth", "Args": { "maximumDestructuringDepth": 4 } },
      { "Name": "ToMaximumCollectionCount", "Args": { "maximumCollectionCount": 10 } },
      {
        "Name": "IgnoreNullValues",
        "Args": {
          "ignoreNullValues": true
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:/logs/ndjson/veriship-.ndjson",
          "rollingInterval": "Day",
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:/logs/veriship/log-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] {RequestId} {Identity} {Message:lj} {RequestBody} {NewLine}{Exception}"
        }
      },
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Indented.Json.JsonFormatter",
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }

      }
    ]
  },
  "ConnectionStrings": {
    "VeriShip": ""
  },
  "Signals": {

    "BaseUrl": "",
    "ApiKey": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
